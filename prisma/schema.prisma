generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

enum LibraryType {
  Tv
  Movies
}

model Library {
  id        String      @id @default(cuid())
  path      String
  type      LibraryType

  createdAt DateTime    @default(now())
  updatedAt DateTime    @updatedAt
}
